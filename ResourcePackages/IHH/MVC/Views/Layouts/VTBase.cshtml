    <title></title>
    <link rel="shortcut icon" href="/ResourcePackages/IHH/assets/dist/images/header/ihh-favicon.webp?1" type="image/png" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport"
    content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1,user-scalable=0">
    <meta name="viewport" content="width=device-width">
    <meta name="format-detection" content="telephone=no">
    <meta name="google" content="notranslate">
    
    @Html.Section("head")
    
    @if (!SystemManager.IsDesignMode)
    {
        @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/bootstrap/bootstrap.min.css"), "head", true)
    }
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/fonts.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/fontawesome/all.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/fontawesome/fontawesome.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/fontawesome/brands.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/fontawesome/solid.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/select2.min.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/daterangepicker.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/slick.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/vendors/slick-theme.css"), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/sidebar.css?ts="+DateTime.Now), "head", true)
    @Html.StyleSheet(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/css/main.css?ts="+DateTime.Now), "head", true)

    @Html.Section("jqueryRef")
    @if (!SystemManager.IsDesignMode)
    {
        @Html.Script(Url.WidgetContent("~/ResourcePackages/IHH/assets/dist/js/vendors/jquery/jquery.min.js"), "jqueryRef")
    }

    </head>

    <!-- Google tag (gtag.js) : Baseline Report -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-H8JV9K6GX7"></script>
